{
  "name": "mdn-web-docs-theme",
  "displayName": "MDN's Web Docs Theme",
  "version": "1.0.0",
  "private": true,
  "description": "A Visual Studio Code theme inspired by MDN's Web Docs",
  "categories": [
    "Themes",
    "Other"
  ],
  "keywords": [
    "Firefox",
    "Light Theme",
    "MDN",
    "MDN Web Docs",
    "Mozilla",
    "Mozilla Developer Network",
    "Theme",
    "VSCode"
  ],
  "homepage": "https://marketplace.visualstudio.com/items?itemName=DanMad.mdn-web-docs-theme",
  "bugs": "https://github.com/DanMad/mdn-web-docs-theme/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/DanMad/mdn-web-docs-theme.git"
  },
  "license": "MIT",
  "author": "DanMad",
  "publisher": "DanMad",
  "scripts": {
    "bootstrap": "yarn install && husky install && chmod +x .husky/*",
    "build": "rm -rf themes && mkdir themes && yarn build:mdn-web-docs-theme",
    "build:mdn-web-docs-theme": "json-stringify src/mdn-web-docs-theme.js -o themes/mdn-web-docs.json",
    "publish": "vsce publish"
  },
  "contributes": {
    "themes": [
      {
        "label": "MDN's Web Docs",
        "path": "./themes/mdn-web-docs.json",
        "uiTheme": "vs"
      }
    ]
  },
  "devDependencies": {
    "husky": "^7.0.1",
    "json-stringify-cli": "^1.0.1",
    "lint-staged": "^11.0.1",
    "prettier": "^2.3.2",
    "sort-package-json": "^1.46.0",
    "vsce": "^1.95.1"
  },
  "engines": {
    "vscode": "^1.42.0"
  },
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#83d0f2",
    "theme": "light"
  }
}
