{
  "name": "mdn-web-docs-theme",
  "displayName": "MDN Web Docs Theme",
  "version": "1.0.0",
  "private": true,
  "description": "A Visual Studio Code theme inspired by MDN's Web Docs",
  "categories": [
    "Themes",
    "Other"
  ],
  "keywords": [
    "Firefox",
    "Light theme",
    "MDN",
    "MDN Web Docs",
    "Mozilla",
    "Mozilla Developer Network",
    "Theme",
    "VSCode"
  ],
  "homepage": "https://github.com/danmad/mdn-web-docs-theme#readme",
  "bugs": {
    "url": "https://github.com/danmad/mdn-web-docs-theme/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danmad/mdn-web-docs-theme"
  },
  "license": "MIT",
  "author": "DanMad",
  "publisher": "DanMad",
  "scripts": {
    "build": "npm run build:mdn-web-docs-theme",
    "build:mdn-web-docs-theme": "json-stringify src/mdn-web-docs-theme.js --out themes/mdn-web-docs.json",
    "package": "npx vsce package",
    "publish": "npx vsce publish",
    "sort-package": "sort-package-json"
  },
  "contributes": {
    "themes": [
      {
        "label": "MDN Web Docs",
        "path": "./themes/mdn-web-docs.json",
        "uiTheme": "vs"
      }
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write"
  },
  "devDependencies": {
    "husky": "^4.3.0",
    "json-stringify-cli": "^1.0.1",
    "lint-staged": "^10.4.2",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.2",
    "sort-package-json": "^1.46.0",
    "vsce": "^1.80.0"
  },
  "engines": {
    "vscode": "^1.42.0"
  },
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#83d0f2",
    "theme": "light"
  }
}
