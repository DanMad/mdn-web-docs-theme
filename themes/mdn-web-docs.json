{
  "colors": {
    "activityBar.background": "#fff",
    "activityBar.border": "#dce3e5",
    "activityBarBadge.background": "#83d0f2",
    "activityBarBadge.foreground": "#333",
    "badge.background": "#83d0f2",
    "badge.foreground": "#333",
    "editor.background": "#eee",
    "editorLineNumber.activeForeground": "#708090",
    "editorLineNumber.foreground": "#999",
    "sideBar.border": "#dce3e5"
  },
  "name": "MDN Web Docs",
  "tokenColors": [
    {
      "name": "Base comments",
      "scope": "comment",
      "settings": { "foreground": "#708090" }
    },
    {
      "name": "Base constants",
      "scope": "constant",
      "settings": { "foreground": "#905" }
    },
    {
      "name": "Base entities",
      "scope": "constant.character.entity",
      "settings": { "foreground": "#9a6e3a" }
    },
    {
      "name": "Base keywords",
      "scope": "keyword",
      "settings": { "foreground": "#07a" }
    },
    {
      "name": "Base storage",
      "scope": "storage",
      "settings": { "foreground": "#07a" }
    },
    {
      "name": "Base strings",
      "scope": "string",
      "settings": { "foreground": "#07a" }
    },
    {
      "name": "Base support",
      "scope": "support",
      "settings": { "foreground": "#333" }
    },
    {
      "name": "Base variables",
      "scope": "variable",
      "settings": { "foreground": "#e90" }
    },
    {
      "name": "CSS attributes",
      "scope": "entity.other.attribute-name.css",
      "settings": { "foreground": "#690" }
    },
    {
      "name": "CSS classes",
      "scope": "entity.other.attribute-name.class.css",
      "settings": { "foreground": "#690" }
    },
    {
      "name": "CSS foreground",
      "scope": "source.css",
      "settings": { "foreground": "#999" }
    },
    {
      "name": "CSS functions",
      "scope": [
        "support.function.calc.css",
        "support.function.gradient.css",
        "support.function.misc.css",
        "support.function.timing-function.css",
        "support.function.transform.css"
      ],
      "settings": { "foreground": "#dd4a68" }
    },
    {
      "name": "CSS hexadecimals",
      "scope": [
        "constant.other.color.rgb-value.hex.css",
        "punctuation.definition.constant.css"
      ],
      "settings": { "foreground": "#333" }
    },
    {
      "name": "CSS ids",
      "scope": "entity.other.attribute-name.id.css",
      "settings": { "foreground": "#690" }
    },
    {
      "name": "CSS important",
      "scope": "keyword.other.important.css",
      "settings": { "fontStyle": "bold", "foreground": "#e90" }
    },
    {
      "name": "CSS operators",
      "scope": [
        "keyword.operator.arithmetic.css",
        "keyword.operator.combinator.css",
        "keyword.operator.pattern.css"
      ],
      "settings": { "foreground": "#9a6e3a" }
    },
    {
      "name": "CSS properties",
      "scope": [
        "support.type.property-name.css",
        "support.type.vendored.property-name.css"
      ],
      "settings": { "foreground": "#905" }
    },
    {
      "name": "CSS property values",
      "scope": [
        "support.constant.color.current.css",
        "support.constant.property-value.css"
      ],
      "settings": { "foreground": "#333" }
    },
    {
      "name": "CSS pseudo",
      "scope": [
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-element.css"
      ],
      "settings": { "foreground": "#690" }
    },
    {
      "name": "CSS strings",
      "scope": ["string.quoted.double.css", "string.quoted.single.css"],
      "settings": { "foreground": "#9a6e3a" }
    },
    {
      "name": "CSS tags",
      "scope": "entity.name.tag.css",
      "settings": { "foreground": "#690" }
    },
    {
      "name": "CSS units",
      "scope": [
        "entity.other.keyframe-offset.percentage.css",
        "keyword.other.unit.%.css",
        "keyword.other.unit.ch.css",
        "keyword.other.unit.cm.css",
        "keyword.other.unit.deg.css",
        "keyword.other.unit.em.css",
        "keyword.other.unit.ex.css",
        "keyword.other.unit.in.css",
        "keyword.other.unit.mm.css",
        "keyword.other.unit.ms.css",
        "keyword.other.unit.pc.css",
        "keyword.other.unit.percentage.css",
        "keyword.other.unit.pt.css",
        "keyword.other.unit.px.css",
        "keyword.other.unit.q.css",
        "keyword.other.unit.rem.css",
        "keyword.other.unit.s.css",
        "keyword.other.unit.vmax.css",
        "keyword.other.unit.vmin.css",
        "keyword.other.unit.vh.css",
        "keyword.other.unit.vw.css"
      ],
      "settings": { "foreground": "#333" }
    },
    {
      "name": "HTML attributes",
      "scope": "entity.other.attribute-name.html",
      "settings": { "foreground": "#690" }
    },
    {
      "name": "HTML brackets",
      "scope": [
        "punctuation.definition.string.begin.html",
        "punctuation.definition.string.end.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.separator.key-value.html"
      ],
      "settings": { "foreground": "#999" }
    },
    {
      "name": "HTML tags",
      "scope": "entity.name.tag.html",
      "settings": { "foreground": "#905" }
    }
  ],
  "type": "light"
}
